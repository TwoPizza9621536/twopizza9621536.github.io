---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
      "excerpt" : {{ post.excerpt | markdownify | strip_html | escape | jsonify }}
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
